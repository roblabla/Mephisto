interface NvidiaService is nvdrv, nvdrv:a, nvdrv:s, nvdrv:t {
	[0] Open(buffer<i8, 5, 0> path) -> (u32 fd, u32 error_code);
	[1] Ioctl(u32 fd, u32 request, buffer<unknown, 0x21, 0> inbuf) -> (u32 error_code, buffer<unknown, 0x22, 0> outbuf);
	[2] Close(u32 fd);
	[3] Initialize(u32 tmemSize, KObject process, KObject transferMemory) -> u32;
}

interface NvidiaDebugger is nvdrvdbg {
}

